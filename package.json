{
  "name": "ui-components",
  "version": "5.3.0",
  "description": "",
  "author": "Brainly Team",
  "license": "GPLv3",
  "module": "esm/index.js",
  "main": "commonjs/index.js",
  "engines": {
    "node": "^6.9.0"
  },
  "files": [
    "src/",
    "esm/",
    "commonjs/"
  ],
  "scripts": {
    "js-lint": "eslint components/**/src/js/",
    "scss-lint": "sass-lint -c node_modules/frontend-tools-configs/.sass-lint.yml  -v -q 'components/**/*.scss'",
    "start": "http-server .",
    "watch": "gulp build && npm start & gulp watch",
    "build": "gulp build",
    "test": "npm run js-lint -s && npm run scss-lint -s",
    "postversion": "git push && git push --tags",
    "package-clean": "yarn rimraf esm/ commonjs/",
    "package-esm": "BABEL_ENV=esm babel components --out-dir esm --only 'components/index.js' --only '**/src/js' --ignore '**/dist/**'",
    "package-commonjs": "BABEL_ENV=commonjs babel components --out-dir commonjs --only 'components/index.js' --only '**/src/js' --ignore '**/dist/**'",
    "package-prepare": "yarn package-clean && concurrently --names 'commonjs,esm' 'yarn run package-commonjs' 'yarn run package-esm'",
    "prepublishOnly": "yarn package-prepare"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "7.1.1",
    "concurrently": "^5.0.2",
    "eslint": "^3.8.1",
    "expose-loader": "^0.7.1",
    "frontend-tools-configs": "github:brainly/frontend-tools-configs#v7.0.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-if": "^2.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.10.2",
    "webpack": "^1.12.15"
  }
}
